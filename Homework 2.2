player_name = input("Welcome to FizzBuzz, please tell me your name:")
num = int(input("Thank you " + player_name + ", please select a number from 1 to 100:"))

for num in range(1,num + 1):
    if num % 3 == 0 and num % 5 == 0:
        print("FizzBuzz")

    elif num % 3 == 0:
        print("Fizz")

    elif num % 5 == 0:
        print("Buzz")

    else:
        print (num)
